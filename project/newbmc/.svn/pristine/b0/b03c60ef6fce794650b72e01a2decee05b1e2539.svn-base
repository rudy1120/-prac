@charset "utf-8";

html,
body,
.wrap{
	height:100%;
	}

/* layout */
.wrap{
	min-width: 1100px;
	position:relative;
	}
.wrap .header{
	min-width: 1100px;
	background:#f5f5f5;
	position: relative;
	padding-left:230px;
	}
.contanier{
	position:relative;
	min-width:1270px;
	background:url(../img/common/bg_sys_snb.png) repeat-y 0 0;
	}
.box_snb{
	width:230px;
	min-height:100%;
	background:#3E4959;
	position:absolute;
	left:0px;
	top:0px;
	z-index:999;
	}
.content{
	margin-left:230px;
	}
.footer{
	position: absolute;
	width: 100%;
	bottom: 0;
	min-width: 1100px;
	height: 50px;
	border-top: 1px solid #c8c8c8;
	}


/* login */
.wrap_login{
	background:url(../img/common/bg_login.gif) repeat 0 0 ;
	position:relative;
	height:100%;
	min-height:100%;
	}
.loginBox{
	width:400px;
	margin:0 auto;
	padding-top:200px;
	}
.loginBox > h1{
	font-weight: 600;
	font-size:30px;
	color:#333333;
	text-align:center;
	line-height:27px;
	letter-spacing:-1px;
	}
.loginBox > p{
	font-family:'Verdana';
	color:#c2c2c2;
	font-size:11px;
	font-weight: 600;
	text-align:center;
	margin-bottom:10px;
	}
.loginBox > div{
	background:url(../img/common/img_login02.png) repeat-y 0 0;
	padding:20px 40px;
	}
.loginBox > img{
	display:block;
	}
.loginBox > div label{
	font-size:11px;
	color:#999999;
	font-weight: 600;
	margin-right:5px;
	}
.loginBox > div > ul,
.loginBox > div > span{
	float:left;
	}
.loginBox > div > span{
	margin-left:15px;
	}
.loginBox > div > ul li{
	line-height:26px;
	text-align:right;
	}
.loginBox > ul{
	margin-left:20px;
	margin-top:10px;
	}
.btn_login{
	background:url(../img/common/btn_login.png) no-repeat 0 0;
	color:#ffffff;
	border:0px;
	font-weight: 600;
	font-size:14px;
	letter-spacing:-1px;
	padding: 15px 15px 20px 8px;
	cursor:pointer;
	}


/* header */
.header > h1{
	font-weight: 600;
	font-size:16px;
	color:#3E4959;
	padding-left:20px;
	padding-top:10px;
	}

.sys_depth01{
	margin:0 auto;
	width:175px;
	}
.sys_depth01 > ul > li{
	float:left;
	}
.sys_depth01 > ul > li > a{
	color:#ffffff;
	display: inline-block;
	padding: 5px 15px;
	border-right:1px solid #000000;
	height:20px;
	font-weight: 600;
	}
.sys_depth01 > ul > li > a.on{
	background:#e36327;
	border-right:0;
	}
.sys_depth01 > ul > li:first-child > a{
	border-left:1px solid #000000;
	border-right:0;
	}
.sys_depth01 > ul > li:first-child > a.on{
	border-left:0px;
	}

.gnb{
	position: absolute;
	right: 4px;
	top: 13px;
	}
.gnb ul li{
	float:left;
	padding-right:10px;
	padding-left:10px;
	}
.gnb ul li:first-child{
	border-left:0px;
	}
.gnb ul li,
.gnb ul li a{
	color:#666666;
	}
.gnb ul li a{
	font-weight: 600;
	}

.lnb{
	border-top:2px solid #65c7ad;
	border-bottom:1px solid #dfdfdf;
	background:#f5f5f5;
	margin-top:15px;
	}
.lnb > ul > li{
	float:left;
	}
.lnb > ul > li > a{
	display:inline-block;
	padding:10px 20px;
	border-right:1px solid #dfdfdf;
	border-bottom:1px solid #dfdfdf;
	font-weight: 600;
	color:#9a9999;
	margin-bottom:-1px;
	}
.lnb > ul > li > a.on{
	background:#ffffff;
	border-bottom:1px solid #ffffff;
	color:#333333;
	}

/* foot */
.footer > p{
	text-align: center;
	padding-top: 10px;
	font-size: 11px;
	color: #888a93;
	}

/* snb */
.box_snb > h2{
	background:#22ae88;
	padding:30px 10px;
	font-weight: 600;
	font-size:18px;
	letter-spacing:-1px;
	color:#ffffff;
	}
.snb > li > a{
	position:relative;
	border-bottom:1px solid #2F3742;
	display:block;
	padding:10px 10px;
	letter-spacing:-1px;
	color:#9EA3AB;
	}
.snb > li:first-child > a{
	border-top:1px solid #2F3742;
	}
.snb > li > a.on{
/* 	background: #485566 url(../img/common/lnb_arrow.png) no-repeat 95% 13px; */
	color:#ffffff;
	font-weight: 600;
	}
.snb > li > a.on:after{
	content:'';
	width:0px;
	height:0px;
	border-left:10px solid #485566;
	border-bottom:20px solid transparent;
	border-top:20px solid transparent;
	position:absolute;
	right:-10px;
	top:0px;
	}
.snb > li > a:hover,
.snb > li > a:focus{
	text-decoration:underline;
	color:#ffffff;
	}
.snb .snb_sub{
	background:#485566;
	padding:10px;
	border-bottom:1px solid #2F3742;
	}
.snb .snb_sub ul > li{
	padding:2px 0px;
	}
.snb .snb_sub ul > li a{
	color:#9EA3AB;
	line-height:18px;
	}
.snb .snb_sub ul > li a.on{
	font-weight: 600;
	color:#ffffff;
	text-decoration:underline;
	}

/* title */
.content  h2,
.content_pop h2{
	background:url(../img/common/bul_blue.png) no-repeat 10px 21px;
	font-size:14px;
	font-weight: 600;
	color:#333333;
	padding:20px 10px 10px 30px;
	}
.content  h3,
.content_pop  h3{
	background:url(../img/common/bul_gray.png) no-repeat 10px 21px;
	font-size:13px;
	font-weight: 600;
	padding:20px 10px 10px 30px;
	}

.content > p,
form > p{
	padding-left:10px;
	}

/* 탭디자인 */
.tab{
	border-bottom: 1px solid #cacaca;
	padding:0 10px;
	}
.tab ul li{
	float:left;
	}
.tab ul li a{
	display:block;
	text-align:center;
	font-size:13px;
	font-weight: 600;
	color:#aeaeae;
	padding:10px 21px;
	}
.tab ul li a.on{
	color:#333333;
	border-top:1px solid #cacaca;
	border-right:1px solid #cacaca;
	border-left:1px solid #cacaca;
	border-bottom:1px solid #ffffff;
	border-top-left-radius:5px;
	border-top-right-radius:5px;
	margin-bottom:-1px;
	padding:10px 20px;
	}


/* tip */
.list_tip{
	margin-bottom:20px;
	margin-left:20px;
	}
.list_tip li{
	background:url(/common/img/common/bul_tip.gif) no-repeat 0 7px;
	padding-left:10px;
	}
.list_tip li b{
	color:#333333;
	}

/* 제목 + 버튼 */
.btn_position{
	position:relative;
	}
.btn_position > a{
	position:absolute;
	right:15px;
	top:13px;
	}
	
/* 개인정보 수집 및 이용안내 */
.bod_privacy{padding: 20px; border: 5px solid #eee; box-sizing:border-box; max-width: 812px;}
.bod_privacy .privacy_tit{color: #333; font-weight: 600; font-size: 20px;}
.bod_privacy strong{color: #333;}