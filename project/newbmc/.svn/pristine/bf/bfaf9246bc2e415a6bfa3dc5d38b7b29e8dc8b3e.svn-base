<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>

	<definition name="/sys/empty/*/*/" extends="sysEmpty">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}.jsp" />
	</definition>
	<definition name="/sys/empty/*/*/*/" extends="sysEmpty">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}/{3}.jsp" />
	</definition>
	<definition name="/sys/empty/*/*/*/*/" extends="sysEmpty">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}/{3}/{4}.jsp" />
	</definition>

	<definition name="/sys/*/*/" extends="sysSub">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}.jsp" />
	</definition>
	<definition name="/sys/*/*/*/" extends="sysSub">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}/{3}.jsp" />
	</definition>
	<definition name="/sys/*/*/*/*/" extends="sysSub">
		<put-attribute name="body" value="/sys/jsp/{1}/{2}/{3}/{4}.jsp" />
	</definition>

</tiles-definitions>