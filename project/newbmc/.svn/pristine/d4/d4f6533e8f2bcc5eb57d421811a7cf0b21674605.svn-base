.mainContInnr {max-width:1200px; margin:0 auto; position:relative;}
.mainContInnr:after {content:""; clear:both; display:block;}

.mainTit .tit {font-weight: lighter; font-size: 2.8em; line-height: 1.2; letter-spacing: -0.5px;}
.mainTit .subtit {font-size: 1.1em; margin:25px 0; letter-spacing: -0.5px; font-weight: lighter; line-height: 1.2;}
.mainTit .subtit strong {display: block; margin-bottom: 20px; font-weight: normal;}

.mainBannerWrap {position: relative;}

.mainBanner .banner img {width:100%;}
.mainBanner .slick-dots {position: absolute; z-index: 999; bottom:20px; width:100%; text-align: center;}
.mainBanner .slick-dots li {display:inline-block; margin:0 5px;}
.mainBanner .slick-dots li button {width:60px; height:14px; position: relative; text-indent: -9999px; background: transparent; border:0;}
.mainBanner .slick-dots li button:before {content:""; display:block; width:100%; height: 3px; background:#fff; position: absolute; top:50%; left:0; transform: translateY(-50%); transition: all 0.3s;}
.mainBanner .slick-dots li.slick-active button:before,
.mainBanner .slick-dots li:hover button:before,
.mainBanner .slick-dots li:focus button:before {height:100%;}

.mainBannerInfo {position: absolute; width:100%; max-width: 1200px; left:50%; top:0; height:100%; transform: translateX(-50%);}
.mainBannerInfo .track {position: absolute; top:0; left:0; height:100%; width:133.3333%;}
.mainBannerInfo .info {position: absolute; z-index: 999; top:0; height:100%; width:calc(25%); color:#fff; text-shadow: 2px 2px 3px rgba(0,0,0,.6);}
.mainBannerInfo .info > div {position: absolute; bottom:calc(25% - 10px); left:30px; opacity: 0; transform: translateY(0); white-space: nowrap; transition: all 0.25s;}
.mainBannerInfo .info > div *:not(button) {display: none;}
.mainBannerInfo .info:before,
.mainBannerInfo .info:after {content:""; display:block; width:1px; height:50%; background:rgba(255,255,255,.4); position: absolute; left:0;}
.mainBannerInfo .info:before {top:0;}
.mainBannerInfo .info:after {bottom:0;}
.mainBannerInfo .info button {display:block; font-size: 2em; font-weight: lighter; color:#fff; text-shadow: 2px 2px 3px rgba(0,0,0,.6); margin-bottom: 10px; border:0; background:0;}
.mainBannerInfo .info .tit {font-size:2.8em; font-weight: normal; margin-bottom: 25px;}
.mainBannerInfo .info .subTit {line-height: 1.4; font-size: 1.2em; font-weight: lighter;}
.mainBannerInfo .info.show > div {opacity: 1; left:20px; bottom:25%;}
.mainBannerInfo .info.active * {display:block;}
.mainBannerInfo .info.active:before,
.mainBannerInfo .info.active:after{background:#fff; height:25%;}
.mainBannerInfo .info.active > div {bottom:50%; left:0 !important; transform: translateY(50%);}
.mainBannerInfo .info.active button {font-size:4em;}

.mainContent2 {background:url(../images/bg_content1_02.png) no-repeat right top; /* height: 520px; */ position: relative;}
.mainContent1 {/* background:url(../images/bg_content1_02.png) no-repeat right top; */ /* height: 520px; */ margin-top:50px; position: relative;}
.mainContent1:before {content:""; display:block; position: absolute; top: 0; left: 0; bottom: 0; width: 100%; max-width: 1350px; background: url(../images/bg_content1_01.png) no-repeat right 280px center;}
.mainContent1 .mainTit {float:left; width:400px; padding-top:150px;}
.mainContent1 .snsList:after {content:""; clear:both; display:block;}
