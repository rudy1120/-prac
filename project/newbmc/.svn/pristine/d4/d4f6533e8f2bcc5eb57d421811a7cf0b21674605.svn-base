.mainContent3 {position: relative; height:520px; margin-top:30px;}
.mainContent3:before {content:""; display:block; position: absolute; top:0; bottom:0; right:0; width:100%; max-width:1200px; background: #f8f8f8;}
.mainContent3:after {content:""; display:block; position: absolute; top:100px; right:1100px; width:200px; height:1px; background:#26c2dd;}
.mainContent3 .mainTit {padding:60px 0 0 580px;}
.mainContent3 .mainCont .img {float:left; width:500px; margin-top:-110px; position: relative;}
.mainContent3 .mainCont .img img {position: absolute; right:55px;}
.mainContent3 .mainCont .cont {float:right; width: calc(100% - 500px); padding-left:80px;}
.mainContent3 .mainCont .cont .btnList:after {content:""; clear:both; display:block;}
.mainContent3 .mainCont .cont .btnList li {float:left; width:calc(33.3333% - 15px); margin-right: 20px; margin-bottom:30px;}
.mainContent3 .mainCont .cont .btnList li > a {display:table; width:100%; height:46px; background:#fff; border:1px solid #ddd; transition: all 0.3s;}
