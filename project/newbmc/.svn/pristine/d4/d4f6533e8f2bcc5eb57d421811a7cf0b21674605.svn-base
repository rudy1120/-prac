/* Fix */
@media all and (max-width:1350px){

    .mainContInnr {padding: 0 20px;}

    .mainBannerInfo {width:calc(100% - 120px); font-size:0.8em;}
    .mainBannerInfo .info .tit {margin-bottom:10px;}
    .mainBannerInfo .info button {margin-bottom:0;}
    .mainBannerInfo .info.show > div {left:10px;}

    
}

/* PC */
@media all and (min-width:1200px){
    .mainContent3 .mainCont .cont .btnList li:nth-of-type(3n) {margin-bottom:0;}
}

/* Tablet & Mobile */
@media all and (max-width:1199px){
    
    #container {font-size:0.9em;}

    .mainContent1 {height:auto;}
    .mainContent1:before {background-color:#fff;}
    .mainContent1 .mainTit {position: absolute; padding-top:0; width:auto; top:50px; left:40px;}
    .mainContent1 .mainCont {width:100%;}
    .mainContent1 .mainCont .list {margin-top:50px;}
    .mainContent1 .mainCont .list li {padding-left:10px; padding-right:10px;}

    .mainContent2 {padding: 80px 0;}

    .mainContent3:before {width:70%;}
    .mainContent3:after {right:70%; top:80px; transform: translateX(50%)}
    .mainContent3 .mainTit {padding:60px 0 0 500px;}
    .mainContent3 .mainCont .img {margin-top:-130px;}
    .mainContent3 .mainCont .img img {width:100%;}
    .mainContent3 .mainCont .cont {padding-left:0;}
    .mainContent3 .mainCont .cont .btnList li {width:calc(50% - 10px); margin-right:20px; margin-bottom:20px;}
    .mainContent3 .mainCont .cont .btnList li:nth-of-type(2n) {margin-right:0;}

    .mainContent4 {padding-top:50px;}
    .mainContent4 .mainEvent {margin:50px 50px 0;}
    .mainContent4 .mainEvent .slick-prev {left:-40px;}
    .mainContent4 .mainEvent .slick-next {right:-40px;}

    .mainContent5 {padding-top:50px;}
    .mainContent5 .mainService {margin: 50px 50px 0;}
    .mainContent5 .mainService .slick-prev {left:-40px;}
    .mainContent5 .mainService .slick-next {right:-40px;}
    
    /* 메인수정 */
    .mainContent2 .mainNotice1,.mainContent2 .mainNotice2{width:100%; padding-bottom:220px;}
}

@media all and (min-width:961px){
    .mainContent1 .mainCont .list li > a:hover,
    .mainContent1 .mainCont .list li > a:focus {/*transform:rotateY(180deg);*/ background:#26c2dd;}
    .mainContent1 .mainCont .list li > a:hover > div,
    /*.mainContent1 .mainCont .list li > a:focus > div {transform: translate(-50%, -50%) rotateY(-180deg)}*/
    .mainContent2 .mainNotice .noticeTab > li > ul > li:nth-of-type(2n + 1):not(.first) {border-left:1px solid #ddd;}	
}

/* Mobile */
@media all and (max-width:960px){
    .mainContInnr {padding:0 40px;}

    .mainBanner .banner {height:400px;}
    .mainBanner .banner img {width:auto; height:100%;}
    .mainBannerInfo .info:not(.active) {visibility: hidden;}

    .mainContent1:before {max-width:none; background-position: right top;}
    .mainContent1 .mainContInnr {padding:0;}
