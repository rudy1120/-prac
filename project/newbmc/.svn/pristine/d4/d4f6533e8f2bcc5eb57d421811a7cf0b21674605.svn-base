.mainContent1 .mainCont .list li > a {display:block; padding-bottom:calc(100% - 20px); position: relative; color:#fff; font-size: 1.1em; font-weight: lighter; line-height: 1.2; transform:rotateY(0); /*transition: all 0.8s;*/}
.mainContent1 .mainCont .list li > a strong {display:block; font-weight: normal; margin-bottom: 10px;}
