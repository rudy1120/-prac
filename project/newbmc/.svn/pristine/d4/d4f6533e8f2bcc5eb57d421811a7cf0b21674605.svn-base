.mainContent1 .mainCont .list li:nth-of-type(1) > a {background: url(../images/bg_list_01.jpg) no-repeat center center / cover;}
.mainContent1 .mainCont .list li:nth-of-type(2) > a {background: #26c2dd url(../images/bg_list_01.jpg) no-repeat center center / cover;} /* 시안에 이미지가 없어서 첫번째 이미지 적용 */
.mainContent1 .mainCont .list li:nth-of-type(3) > a {background: url(../images/bg_list_03.jpg) no-repeat center center / cover;}
.mainContent1 .mainCont .list li:nth-of-type(4) > a {background: url(../images/bg_list_04.jpg) no-repeat center center / cover;}
.mainContent1 .mainCont .list li:nth-of-type(5) > a {background: url(../images/bg_list_05.jpg) no-repeat center center / cover;}
.mainContent1 .mainCont .list li > a > div {position: absolute; text-align: center; left:50%; top:50%; transform: translate(-50%, -50%); width: 100%; transition: all 0.8s;}
.mainContent1 .mainCont .list li > a > div:before {content:""; display: inline-block; width:60px; height: 60px; margin-bottom: 5px;}
.mainContent1 .mainCont .list li:nth-of-type(1) > a > div:before {background: url(../images/ico_main_01.png) no-repeat center center;}
.mainContent1 .mainCont .list li:nth-of-type(2) > a > div:before {background: url(../images/ico_main_02.png) no-repeat center center;}
.mainContent1 .mainCont .list li:nth-of-type(3) > a > div:before {background: url(../images/ico_main_03.png) no-repeat center center;}
.mainContent1 .mainCont .list li:nth-of-type(4) > a > div:before {background: url(../images/ico_main_04.png) no-repeat center center;}
.mainContent1 .mainCont .list li:nth-of-type(5) > a > div:before {background: url(../images/ico_main_05.png) no-repeat center center;}
.mainContent1 .mainCont .dots {display:none; position: absolute; bottom: 20px; width: 100%; text-align: center;}
.mainContent1 .mainCont .dots > li {display: inline-block; margin:0 3px;}
.mainContent1 .mainCont .dots > li button {text-indent: -9999px; font-size: 0; background:transparent; border:2px solid #fff; width:14px; height:14px;}
.mainContent1 .mainCont .dots > li.active button {background:#fff;}

