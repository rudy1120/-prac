.mainContent1 .snsList li > a {display:block; text-indent: -9999px; font-size: 0; width:50px; height:50px; border-radius: 50%;/* transform: rotateY(0);*/ transition: all 0.3s;}
.mainContent1 .snsList li > a:hover,
.mainContent1 .snsList li > a:focus {transform: rotateY(180deg);}
