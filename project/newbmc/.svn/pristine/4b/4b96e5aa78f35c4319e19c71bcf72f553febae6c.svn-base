/* 1. About BMC */
/* s0101 */
.conts_txt.cf { position: relative; }
.conts_txt.cf .symbol_img {
	width: 300px; height: 200px; padding: 20px 20px;
	float: left; margin-right: 20px;
	text-align: center;
}
.conts_txt.cf .symbol_txt {
	width: calc(100% - 360px); height: 200px; 
	float: right; line-height: 1.5;
	/* border: 1px solid #e7e7e7; */
	display: table;
	position: relative;
}
.conts_txt.cf .symbol_txt::after {
	content: '';
	position: absolute;
    top: 60px; left: -60px;
    width: 1px; height: 80px; background: #e1e1e1;
}
.conts_txt.cf .symbol_txt span {
	display: table-cell;
    vertical-align: middle;
}

.box_option {
	position: relative;
	float: right;
	clear: both;
	width: 100%; margin-top: -80px !important;
	text-align: right;
}
.box_option .btn {
	position: relative;
	width: 220px !important; height: 50px;
	font-size: 1.8rem;
	padding: 10px 0;
}
.box_option .btn:not(.CI) a {
	display: block;
	width: 100%; height: 100%; padding-left: 40px; text-align: left;
}
.box_option .btn:not(.CI):after {
	content: "";
	position: absolute;
	top: 50%; right: 0%; transform: translateY(-50%);
	width: 18px; height: 17px;
	background-image: url('../img/sub/icon/down_icon.png');
	background-repeat: no-repeat;
	padding-right: 40px;
}
.box_option .btn.CI {
	margin-left: 10px;
}
.box_option .btn.CI a {
	display: block;
	width: 100%; height: 100%; padding-left: 30px; text-align: left;
}
.box_option .btn.CI:after {
	content: "";
	position: absolute;
	top: 50%; right: 0%; transform: translateY(-50%);
	width: 18px; height: 17px;
	background-image: url('../img/sub/icon/down_icon.png');
	background-repeat: no-repeat;
	padding-right: 30px;
}
.btn-down {
	color: #136bd8 !important;
	background-color: rgb(0, 0, 0, 0);
	border-color: #aacaf3 !important;
	border-radius: 40px;
}

.company_box {
	width: 1400px; height: auto;
	margin: 0 auto;
	text-align: center;
}
.company_box span.mb-20 { display: block; }

.company_box .bc_sec {
	float: left;
	width: 320px; height: 360px; margin-top: -20px;
	box-shadow: 0px 13px 10px -9px rgb(0, 0, 0, 40%);
