/* 1. About BMC */
/* s0102 */
.ceo_box {
	width: 1400px; height: 470px; margin: 0px auto 40px;
	text-align: center;
	overflow: hidden;
	position: relative;
}
.ceo_box::after {
	content: '';
	position: absolute; bottom: 0; left: 0;
	width: 1400px; height: 260px;
	background-color: #f5f5f5;
	z-index: -1;
}

.ceoImg {
	/*
	position: absolute;
	top: 0px; right: 0;
	*/
	transition:all 0.3s;
	z-index: 1;
}
.ceoImg p.txtBg {
	position: absolute; bottom: 0; right: 0;
	width: 600px; height: 100px; line-height: 100px; 
	background-color: rgb(0, 77, 194, 0.7); 
	-webkit-backdrop-filter: blur(10px);
	backdrop-filter: blur(10px);
	transform: skew(-20deg) translate(30px, 0px);
	z-index: 2;
}
.ceoImg p.txtBg span {
	display: block; transform: skew(20deg); 
	padding-left: 60px;
	font-size: 2.2rem; color: #fff; 
}
.ceoImg img { 
	position: absolute; bottom: 0; right: 60px;
	display: block; 
}
.ceo_box h3 {
	position: relative;
	width: 760px;
	display: block;
	font-size: 4.4rem; font-weight: 700; font-family: 'Cardo', serif;
	text-align: left; line-height: 1.4;
	padding-left: 60px; margin-top: 60px;
}
.ceo_box h3:before {
	content: "";
	position: absolute;
	top: -11%; left: 0; transform: translateY(-50%);
	width: 34px; height: 31px;
	background: url('../img/sub/ceo_obj1.png');
}
.ceo_box h3:after {
	content: "";
	position: absolute;
	top: 83%; right: 0%;
	transform: translateY(-50%);
	width: 34px; height: 31px;
	background:url('../img/sub/ceo_obj2.png');
}
.ceo_box h3 span { display: block; }
.ceo_box p.box { 
	text-align: left; width: 45%; float: left; 
	font-size: 2.0rem; line-height: 1.3;
	padding-left: 65px; margin-top: 80px;
}

.ceo_txt p.txtType2:last-child { 
	font-weight: 400; text-align: right; 
	padding-top: 20px; padding-right: 135px; 
	position: relative; 
}
.ceo_txt p.txtType2:last-child img { position: absolute; right: 0; top: 0; }

@media screen and (max-width: 1400px){
	.ceo_box { width:100%; }
	.ceo_box::after { width: 100%; }
}
@media screen and (max-width: 1280px){
	.ceo_box h3 { width: 700px; font-size: 4rem; }
	.ceo_box::after { height: 280px; }
	.ceoImg img { width: 30%; }
	.ceoImg p.txtBg { width: 500px }
}
@media screen and (max-width: 1024px){
	.conts_txt p.txtType2 { font-size: 1.6rem; margin-bottom: 20px; }

	.ceo_box { height: 320px;  }
	.ceo_box::after { height: 155px; }
	.ceo_box h3 { width: 100%; margin-top: 20px; font-size: 3rem; padding-left: 2.5%; }
	.ceo_box h3:before, 
	.ceo_box h3:after { content: none; }
	.ceo_box p.box { width: 70%; padding-left: 2.5%; margin-top: 60px; font-size: 1.6rem; }
	.ceoImg img { width: 25%; right: 2.5%; }
	.ceoImg p.txtBg { width: 40%; height: 80px; line-height: 80px; }
	.ceoImg p.txtBg span { font-size: 1.8rem; padding-left: 0px; margin-right: 15%; text-align: right; }
}
@media screen and (max-width: 768px){
	.ceoImg p.txtBg { display: none; }
}
@media screen and (max-width: 560px){
	.ceo_box { height: 250px; }
	.ceo_box::after { height: 150px; }
	.ceo_box h3 { font-size: 2.6rem; line-height: 1.3; }
	.ceo_box p.box { margin-top: 40px; }
}
@media screen and (max-width: 500px){
	.ceo_box { height: auto; overflow: auto; }
	.ceo_box::after { height: 90px; }
	.ceo_box h3, .ceo_box p.box { padding-left: 0;}
	.ceo_box p.box { width: 100%; margin-top: 20px; }
	.ceoImg { padding-top: 80px; }
	.ceoImg img { position: static; width: 50%; padding-left: 4%; }
	.ceoImg p.txtBg { display: block; width: 100%; height: auto; line-height: normal; background-color: initial; transform: none; backdrop-filter: initial;}
	.ceoImg p.txtBg span { color: #111; transform: none;  margin: 0; padding: 0 4% 20px; line-height: 1.3; }
	.ceoImg p.txtBg span .textYellow { display: block; font-weight: 500; color: #127ce0; }
	.ceo_txt p.txtType2:last-child { text-align: left; }
	.ceo_txt p.txtType2:last-child span { display: block; }
	.ceo_txt p.txtType2:last-child img { top: auto; bottom: 0; }
}
@media screen and (max-width: 300px){
	.ceo_box h3 {line-height: 1.1;}
}



