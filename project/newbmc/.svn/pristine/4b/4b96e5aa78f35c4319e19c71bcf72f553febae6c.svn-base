	transition: all 0.3s; 
}
.company_box .box_bg {
	width: calc(100% - 320px); height: 340px;
	margin-top: 40px; margin-left: 340px; padding: 30px 40px;
	text-align: left;
	background: #f5f5f5;
	z-index: 0;
}

.box_bg h3 { font-size: 2.4rem; font-weight: 500; line-height: 1.4; }
.box_bg h3 span {
	display: block;
	font-size: 4.4rem; font-weight: 700; font-family: 'Cardo', serif; color: #127ce0; 
	margin-top: 10px; margin-bottom: 10px;
}
i.point { color: #127ce0; }
.company_box .box_bg > h4 {
	margin-bottom: 20px; margin-top: 20px;
	font-size: 2rem; line-height: 1.3;
}
.box_bg > span {
	font-size: 1.8rem; line-height: 1.2;
}
.layout_box {
	position: relative;
	width: 100%; height: 400px;
	background: url('../img/sub/ab_symbol_bg.png'); 
	border: 1px solid #ddd;
}
.layout_box img {
	position: absolute;
	top: 43%; left: 50%; transform: translate(-50%, -50%);
}

