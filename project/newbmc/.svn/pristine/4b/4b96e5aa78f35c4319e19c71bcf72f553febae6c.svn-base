@charset "UTF-8";
#contents {
	position: relative;
	*zoom:1;
}
#contents:after {
	content: "";
	display: block;
	clear: both;
}
.sVisual {
	height: 380px;
	position: relative;
	overflow: hidden;
	margin-top: 0px;
}
.sVisual:before {
	position: absolute;
	left: 50%;
	top: 45%;
	transform: translate(-50%, -50%) scale(1.3);
	content: "";
	width: 100%;
	height: 100%;
	transition: all 5s .5s;
}
.sVisual.on:before { transform: translate(-50%, -50%) scale(1.1); }
.aboutBmc:before { background: url('../img/sub/sVs01.png') no-repeat center center/cover; }
.bmcAc:before { background: url('../img/sub/sVs02.png') no-repeat center center/cover; }
.bmcProjects:before { background: url('../img/sub/sVs03.png') no-repeat center center/cover; }
.bmcWp:before { background: url('../img/sub/sVs05.png') no-repeat center center/cover; }
.bmcCity:before { background: url('../img/sub/sVs06.png') no-repeat center center/cover; }

.bmcESG:before { background: url('../img/sub/sVs04.png') no-repeat center center/cover; }
.bgNon:before {content:none !important;}
.bgNon:after {content:none !important;}
.svInner {
	position: absolute; top: 50%; left: 50%;
	transform: translate(-50%, -20%);
	width: 100%;
}
.svInner h3 {
	font-size: 5.4rem; font-family: 'Cardo', serif; text-align:center;
	/* line-height: 5rem; */ color: #fff;
}
.svInner ul {
	width: 100%; margin: 0 auto;
	padding-top: 20px;
	text-align: center;
}
.svInner ul li {
	position: relative;
	display: inline;
	font-size: 1.7rem; text-align: center; color: #fff;
}
.svInner ul li:not(:first-child) { padding-left: 33px }
.svInner ul li:not(:first-child):after {
	content: "〉";
	position: absolute; top: 29%; left: 13px; transform: translateY(-50%);
	width: 7px; height: 7px;
	font-size: 1.1rem; color: #fff;
}
.sNav {
	position: relative;
	width: 1400px; height: 70px;
	margin: -40px auto 0;
	background: #0067c8;
	text-align: center;
	box-shadow: 3px 5px 10px rgb(0, 0, 0, 10%);
	overflow: hidden;
	transition: 0.4s;
}
.sNav.active { height: auto; }
.sNav ul { display: flex; flex-wrap: nowrap; }
.sNav li:first-child {
	width: 70px !important; height: 70px;
	background: #fff
}
.sNav li:nth-child(2) { display: none; }
.sNav li {
	position: relative;
	width: calc(1370px / 4);
	text-align: center;
}
.sNav li a {
	font-size: 1.7rem; color: #fff;
	padding: 21px 10px;
}
.sNav li a:hover, .sNav li a:focus, .sNav li a:active { color: #fff; }
.nav01 li:nth-child(n+3):nth-child(-n+5):after {
	content: "";
	position: absolute; top: 50%; right: 0%; transform: translateY(-50%);
	width: 2px; height: 19px;
	background: #599cdb
}
.nav02 li:nth-child(n+3):nth-child(-n+5):after {
	content: "";
	position: absolute; top: 50%; right: 0%; transform: translateY(-50%);
	width: 2px; height: 19px;
	background: #599cdb
}
.nav03 li {width: calc(1370px / 3);}
.nav03 li:nth-child(n+3):nth-child(-n+4):after {
	content: "";
	position: absolute; top: 50%; right: 0%; transform: translateY(-50%);
	width: 2px; height: 19px;
	background: #599cdb;
}


.conts { width: 1400px; margin: 60px auto; }
.conts h4 {
	position: relative;
	font-size: 2.6rem; font-weight: 500; line-height: 3.5rem;
	margin-top: 50px;
}
.conts h4:after {
	content: "";
	position: absolute; top: -5px; left: 0%; transform: translateY(-50%);
	width: 6px; height: 6px;
	background: #127ce0;
}



.conts22 { margin: 80px auto; }
.conts22 h4 {
	position: relative;
	font-size: 3.2rem; font-weight: 500; line-height: 3.5rem;
	margin-top: 110px;
}
.conts22 h4:after {
	content: "";
	position: absolute; top: -32%; left: 0%; transform: translateY(-50%);
	width: 8px; height: 8px;
	background: #127ce0;
}
.conts22 > h4 ~ p {
	display: block;
	width: 100%;
	border-top: 1px solid #ddd;
	margin-top: 20px;
	padding-top: 15px;
	font-size: 1.7rem;
}



