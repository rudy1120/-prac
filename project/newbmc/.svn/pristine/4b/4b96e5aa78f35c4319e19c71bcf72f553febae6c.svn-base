.timeline {
	position: relative;
	max-width: 1400px;
	margin: 0 auto;
}
.box_left {
  padding-right: 40px;
  position: absolute;
  width: 50%;
}
.box_left p { font-size: 1.6rem; font-weight: 400; color: #000; line-height: 1.5; /*word-break: break-all; */ word-break: keep-all; }

.box_right {  
	width: 50%; left: 50%;
	padding-left: 40px;
	
	position: relative;
}
.box_right::after {
	content: '';
	position: absolute;
	top: 30px; bottom: 0; left: 0;
	width: 4px; margin-left: -3px;
	background-color: #e9e9e9;
}
.box_right h2 {
	font-size: 2.4rem; font-weight: 500; color: #111;
	margin-bottom: 5px;
	position: relative;
}
.box_right h2::after {    
	content: '';
    position: absolute; top: 10px; left: -50px;
    width: 11px; height: 11px;
    background-color: white;
    border: 3px solid #127ce0; border-radius: 50%;
    z-index: 2;
}

.box_right p {
	font-size: 1.6rem; font-weight: 400; color: #555; line-height: 1.2;
	word-break: break-word;
	padding-bottom: 20px;
	position: relative;
}
.box_right p:last-of-type { padding-bottom: 0; }
.box_right .ht_cont:last-of-type p:last-of-type { padding-bottom: 5px; }

.box_right .ht_cont p::after, 
.box_right .awards_date dt::after {
	content: '';
    position: absolute; top: 5px; left: -47px;
    width: 6px; height: 6px;
	background-color: #e1e1e1;
	border: 3px solid #fff;
    border-radius: 50%;
    z-index: 2;
}

