    #header #topNav ul.topNavMenus {width:100%; background:#333;}
    #header #topNav ul.topNavMenus > li {position:relative; border-bottom:1px solid #666;}
    #header #topNav ul.topNavMenus > li.topNavArea {padding-bottom:20px;}
    #header #topNav ul.topNavMenus > li a {display:block; position:relative;}
    #header #topNav ul.topNavMenus > li > a {line-height:60px; text-decoration:none; color:#fff; padding-left:20px;}
    #header #topNav ul.topNavMenus > li > a > strong {padding:5px 0 0 20px; color:#fff; font-weight:500;}
    #header #topNav ul.topNavMenus > li > .btnMenuDropDown {position:absolute; top:0; right:0;}
    #header #topNav ul.topNavMenus > li.active.activeHover > a {color:#fff; background:#666;}
    #header #topNav ul.topNavMenus > li.active.activeHover > .btnMenuDropDown:before, #header #topNav ul.topNavMenus > li.hover > .btnMenuDropDown:before {background-position:-120px 0;}
    #header #topNav ul.topNavMenus > li.hover > a {color:#fff;}
    #header #topNav ul.topNavMenus > li.hover .topNavSub {display:block;}
    #header #topNav ul.topNavMenus > li .topNavSub.areaType {display:block;}
    #header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li {width:25%;}
    #header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li > a {padding:0; font-size:0.889rem;}
    
    /* s : gnb mobile */
    #header #topNav ul.topNavMenus .btnMenuDropDown {display:block; overflow:hidden; position:absolute; top:0; right:0; text-indent:-9999px;}
    #header #topNav ul.topNavMenus > li > .btnMenuDropDown {width:50px; height:60px; text-align:center; display:block; color:#fff;}
    #header #topNav ul.topNavMenus > li.active.activeHover .topNavSub {display:block; background:#f4f4f4; padding:20px 0;}
    #header #topNav ul.topNavMenus > li.hover > a {background:#00879e; color:#fff;}
    #header #topNav ul.topNavMenus > li.hover .topNavSub {display:block;}
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub {display:none;}
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li {display:block; height:auto !important; position:relative;}
    #header #topNav ul.topNavMenus > li .topNavSub li {position:relative;}      
