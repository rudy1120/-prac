@charset "utf-8";

/* 슬라이드 배너 */

#MainBanner {position:relative; }
.SlideArea {position:absolute; top:20%; text-align:center; z-index:9; color:#fff; width:100%;}
.SlideArea h2 {font-size:5.556rem; line-height:110%; font-weight:lighter; letter-spacing:-7px;  white-space:pre-line; margin-bottom:30px; }
.SlideArea h2:before {content: ''; display:block; width:1px; height:45px; background-color:#fff; margin:0 auto; margin-bottom:20px;}
.SlideArea p {font-weight:lighter; font-size:1.333rem; line-height:40px; }

.swiper-container {width:100%; height:800px; margin:0 auto; position:relative; overflow:hidden; list-style:none; padding:0; z-index:1;}
.swiper-container .swiper-slide h2 {position:relative; display:inline-block; font-size:3.444rem; line-height:70px; color:#fff; margin-top:200px;}
.swiper-container .swiper-slide h2:after {content: ''; width:100%; height:1px; background:rgba(255,255,255,0.3); position:absolute; left:0; bottom:-35px;}
.swiper-container .swiper-slide h2 em {display:block; font-size:3.333rem; font-weight:100;}
.swiper-container .swiper-slide p {font-size:1.5rem; color:#fff; }

.ss1 img {width:100%; height:100%; background-size:cover;} 

/* IR 효과 */
/*.ir_pm {display:block; overflow:hidden; font-size:0; line-height:0; text-indent:-9999px;} /* 의미있는 이미지의 대체 텍스트를 제공하는 경우(Phark Method) */
.ir_wa {display:block; overflow:hidden; position:relative; z-index:-1; width:100%; height:100%;} /* 의미있는 이미지의 대체 텍스트로 이미지가 없어도 대체 텍스트를 보여주고자 할 때(WA IR) */
.ir_so {overflow:hidden; position:absolute; width:0; height:0; line-height:0; text-indent:-9999px;} /* 대체 텍스트가 아닌 접근성을 위한 숨김 텍스트를 제공할 때 */


/* Header 영역 */
#header {width:100%; height:100%;} 
#header .headerInnr {max-width:1200px; margin:0 auto;}
#header .headerInnr:after {content:''; display:block; clear:both;}
#header .headerBody {position:relative; height:90px; background:#fff; border-bottom:1px solid #ddd;}
#header .headerBody:after {content:""; clear:both; display:block;}

/* Logo */
#header .logo {float:left; margin-top:15px;}

/* HamBTN */
#header .btnNav {position:relative; display:inline-block; vertical-align:top; width:70px; height:100%; text-indent:-9999px;}
#header .btnNav > span {position:absolute; display:block; width:22px; height:22px; left:50%; top:50%; transform:translate(-50%, -50%);}
#header .btnNav .btnBar {position:absolute; left:0; display:block; width:100%; height:2px; background:#333; opacity:1; transition: all 0.2s;}
#header .btnNav .btnBar:nth-of-type(1) {top:0;}
#header .btnNav .btnBar:nth-of-type(2) {top:10px;}
#header .btnNav .btnBar:nth-of-type(3) {top:20px;}
#header .btnNav.opened .btnBar:nth-of-type(1) {transform: rotate(45deg); top:50%;}
#header .btnNav.opened .btnBar:nth-of-type(2) {left:-50px; opacity:0;}
#header .btnNav.opened .btnBar:nth-of-type(3) {transform: rotate(-45deg); top:50%;}

/* 메뉴 */
#header #topNav {z-index:1000; float:right; position:relative;}
#header #topNav ul.topNavMenus > li > .btnMenuDropDown {position:relative; display:block; width:54px; height:40px; text-align:center; text-indent:-9999px; transition:all 0.25s;}
#header #topNav ul.topNavMenus > li > .btnMenuDropDown:before {content:''; position:absolute; top:50%; left:50%; margin:-9px 0 0 -9px; width:18px; height:18px; background:url('../img/sp_ico_header.png') no-repeat -90px 0; background-size:150px auto;}

#header #topNav ul.topNavMenus > li .topNavSub.areaType > ul:after {background:#000; content:""; clear:both; display:block;}
#header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li {display:block; width:11.11111%; float:left; height:114px; padding:0;}
#header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li > a {display:block; text-align:center; color:#333; font-size:0.889rem; line-height:1.2; text-decoration:none; transition:all 0.2s;}
#header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li > a:hover,
#header #topNav ul.topNavMenus > li .topNavSub.areaType > ul > li > a:focus {margin-top:-5px;}

#header #topNav > .btnClose {position:absolute; background-color:#fff;}
#header #topNav ul.topNavMenus > li > .btnMenuDropDown + .topNavSub .btnMenuDropDown {text-indent:-99999px;}
#header #topNav ul.topNavMenus > li > .btnMenuDropDown {transition:all 0.25s;}
#header #topNav ul.topNavMenus > li > .btnMenuDropDown:hover, #header #topNav ul.topNavMenus > li > .btnMenuDropDown:focus {transform:rotate(180deg);}

#header .topNavBackDv.opened {display:block; z-index:99;}

/* Main_content 영역 */
.Main_wrap {max-width:100%; text-align:center; margin-bottom:100px;}
.Main_wrap .Main_title {max-width:1200px; margin:80px auto;}
.Main_wrap .Main_title:after {content:''; display:block; clear:both; border-bottom:1px solid #999; width:100%; margin:0 auto; margin-top:70px;}
.Main_wrap .Main_title p {letter-spacing:5px; font-weight:lighter;}
.Main_wrap .Main_title h2 {font-weight:lighter; font-size:4.444rem; padding-top:40px; letter-spacing:-4px; }
.Main_wrap .Main_title h2 strong {font-weight:bold; }

.Main_businessBg {width:50%; height:100%; background:url(../img/main/main_business01.jpg) no-repeat left top; position:absolute; top:180px; right:955px;}
.Main_businessWrap_mobile {display:none;}
.MainContent_wrap01 {position:relative; margin-bottom:100px;}
.MainContent_wrap01:after {content:''; display:block; clear:both;}
.MainContent_wrap01 dl {position:relative;}
.MainContent_wrap01 dl dt {font-weight:lighter; font-size:2.667rem;}
.MainContent_wrap01 dl dd {font-weight:lighter; margin-top:45px; line-height:1.7; white-space:pre-line;} 
.MainContent_wrap01 dl dd:nth-of-type(2) {position:relative; margin-top:35px; font-size:0.889rem; color:#666;}
.MainContent_wrap01 dl dd:nth-of-type(2):before {content:""; display:block; clear:both; position:absolute; top:-15px; left:0; width:30px; height:1px; background:#666; }

.MainContent_wrap01 .Main_businessImg {overflow:hidden; height:245px; margin:0 auto; margin-top:30px; position:relative;}
.MainContent_wrap01 .Main_businessImg img {width:100%; height:100%; object-fit:cover; transform:scale(1.0); transition:transform .5s;}
.MainContent_wrap01 .Main_businessImg img:hover {transform:scale(1.2); transition:transform .5s;}

.MainContent_wrap01 .Main_businessWrap {max-width:1200px; height:100%; text-align:left; letter-spacing:-1px; margin:0 auto;}
.MainContent_wrap01 .Main_businessWrap a {position:absolute; bottom:-45px; left:50%; margin-left:-33px;}

.MainContent_wrap01 .Main_business01 {width:32%; height:auto; float:left; text-align:center; position:relative;}
.MainContent_wrap01 .Main_business01 dt {font-size:2.778rem; font-weight:lighter; color:#006fa2; letter-spacing:-3px; margin-top:-20px; line-height:45px;}
.MainContent_wrap01 .Main_business01 dt p {font-size:1.111rem; margin-bottom:10px; font-weight:bold; color:#333; letter-spacing:0;}
.MainContent_wrap01 .Main_business01 dt span {font-size:3.889rem; color:#004768;}
.MainContent_wrap01 .Main_business01 dl dd {margin-top:30px;}
.MainContent_wrap01 .Main_business01 dl dd:nth-of-type(2) {position:relative; padding-bottom:30px; margin-bottom:30px;}
.MainContent_wrap01 .Main_business01 dl dd:nth-of-type(2):before {content:""; display:block; clear:both; position:absolute; top:-15px; left:50%; margin-left:-15px; width:30px; height:1px; background:#666;}

.MainContent_wrap01 .Main_business02 {width:32%; margin-left:25px; float:left; position:relative;}
.MainContent_wrap01 .Main_business03 {width:32%; float:right; position:relative;}
.MainContent_wrap01 .Main_business04 {width:32%; float:left; margin-top:100px; position:relative;}
.MainContent_wrap01 .Main_business05 {width:32%; margin-left:25px; float:left; margin-top:100px; position:relative;}
.MainContent_wrap01 .Main_business06 {width:32%; float:right; margin-top:100px; position:relative;}

.MainContent_wrap02 {width:100%; height:750px; color:#fff; font-weight:lighter; background:url(../img/main/main_plan.jpg) no-repeat center;}
.MainContent_wrap02 .MainContent_plan {padding-top:30px; }
.MainContent_wrap02 .MainContent_plan .txt01 {margin-bottom:40px; line-height:1.5; white-space:pre-line;}
.MainContent_wrap02 .MainContent_plan .txt02 {margin-top:40px; white-space:pre-line;}

.MainContent_wrap03 {width:100%; height:100%;}
.MainContent_wrap03 .MainContent_historyWrap {max-width:1200px; margin:0 auto;}

.MainContent_history > .history_tab {width:60%; float:right; }
.MainContent_history > .history_tab a {cursor:pointer;} 
.MainContent_history > .history_tab ul {margin-bottom:50px; height:60px; background:#f4f4f4;}
.MainContent_history > .history_tab ul li{border-bottom:1px solid #ccc;}
.MainContent_history > .history_tab ul li:first-child {border:2px solid #666; background:#fff;}
.MainContent_history > .Main_historyTxt {width:40%; float:left;}
.MainContent_history > .Main_historyTxt h4 {text-align:left; font-size:2rem;}
.MainContent_history > .Main_historyTxt dl {background:url(../img/intro/history_bg.png) 40px 0 repeat-y; margin-top:20px; height:130px;}
.MainContent_history > .Main_historyTxt dl dt {background:url(../img/main/main_hexagon.png) no-repeat 0 0; width:79px; height:89px; line-height:89px; color:#fff; font-size:1.333rem; float:left;}
.MainContent_history > .Main_historyTxt dl dd {text-align:left; margin-left:110px; padding-top:15px;}

.Main_historyImgBox {position:relative; clear:both;}
.Main_historyImgBox .Main_historyImgBox_img {overflow:hidden; height:286px;}
.Main_historyImgBox .Main_historyImgBox_img img {min-height:200px; width:100%; height:100%; object-fit:cover; transform:scale(1.0); transition:transform .5s;}
.Main_historyImgBox .Main_historyImgBox_img img:hover {transform:scale(1.2); transition:transform .5s;}
.Main_historyImgBox a {position:absolute; top:40%; left:50%; margin-left:-33px;}

.MainContent_wrap04 {width:100%; height:100%; position:relative;}
.MainContent_wrap04:after {content:""; display:block; clear:both;}
.MainContent_notice {max-width:1200px; margin:0 auto;}
.MainContent_notice h3 {font-size:1.667rem; text-align:left;}

.MainContent_notice .main_noticeWrap {width:65%; float:left; position:relative;}
.MainContent_notice .main_noticeWrap img {position:absolute; top:0; right:0;}
.MainContent_notice .main_noticeWrap .main_notice_tit {height:30px;}
.MainContent_notice .main_noticeWrap .main_notice_tit h3 {float:left;}
.MainContent_notice .main_noticeWrap .main_notice_tit a {float:right; width:30px; height:30px;}
.MainContent_notice .main_noticeWrap .main_notice_top {text-align:left; padding:30px 40px; background:#f4f4f4; margin-top:30px;}
.MainContent_notice .main_noticeWrap .main_notice_top h5 {font-size:1.333rem; font-weight:normal;}
.MainContent_notice .main_noticeWrap .main_notice_top p.con_txt {font-size:0.889rem; color:#666; font-weight:lighter; margin-top:20px; height:50px; overflow:hidden; word-wrap:break-word;
  display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical; }
.MainContent_notice .main_noticeWrap .main_notice_top p.con_daytxt {font-size:0.778rem; color:#666; font-weight:lighter; margin-top:30px;}

.MainContent_notice .main_scheduleWrap {width:30%; float:right; position:relative;}
.MainContent_notice .main_scheduleWrap img  {position:absolute; top:0; right:0;}
.MainContent_notice .main_scheduleWrap .main_schedule_tit {height:30px;}
.MainContent_notice .main_scheduleWrap .main_schedule_tit h3 {float:left;}
.MainContent_notice .main_scheduleWrap .main_schedule_tit a {float:right; width:30px; height:30px;}

.main_schedule_top {margin-top:32px; text-align:left; }
.main_schedule_top ul li:nth-of-type(1) {width:60px; height:60px; background:url('../img/main/schedule_ico.png') no-repeat; float:left; margin-right:20px;}
.main_schedule_top ul li:nth-of-type(2) {font-size:1.111rem; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.main_schedule_top ul li:nth-of-type(3) {font-size:0.778rem; color:#666; margin-top:5px; }

.main_notice_bottom { padding:30px 40px 0 40px; font-size:0.889rem; }
.main_notice_bottom ul{text-align:left; line-height:30px;}
.main_notice_bottom ul li .date {float:right; color:#666;}
.main_notice_bottom ul li .tit {overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}

.main_schedule_bottom {width:100%; margin-top:30px; font-size:0.889rem; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.main_schedule_bottom li {border-top:1px solid #ccc; padding:15px 0; text-align:left;}
.main_schedule_bottom li:nth-of-type(1) {border-top:3px solid #ccc;}

.main_schedule_bottom .txt {padding-top:10px; text-align:left; font-size:0.889rem;}
.main_schedule_bottom  ul li .date {float:right; color:#666;}


/* Footer 영역 */
#footer {width:100%; height:100%; word-break:keep-all;}
#footer_top {background-color:#626262; width:100%; height:100%; }
.footer_top_wrap {position:relative; width:1200px; height:65px; margin:0 auto; line-height:65px;}
.footer_top_wrap .footer_sns {float:left; margin-top:20px; }
.footer_top_wrap .footer_sns ul li {width:31px; height:22px; overflow:hidden; cursor:pointer; vertical-align:middle; margin:0 10px; float:left;}
.footer_top_wrap .footer_sns ul li a {display:block;}
.footer_top_wrap .footer_sns ul li.s1 {background-position: 0px 0px; }
.footer_top_wrap .footer_sns ul li.s2 {background-position: -31px 0px;}
.footer_top_wrap .footer_sns ul li.s3 {background-position: -62px 0px;}
.footer_top_wrap .footer_sns ul li.s4 {background-position: -93px 0px;}
.footer_top_wrap .footer_sns ul li.s5 {background-position: -124px 0px;}
.footer_top_wrap .footer_sns ul li.s1:hover {background-position: 0 -22px;}
.footer_top_wrap .footer_sns ul li.s2:hover {background-position: -31px -22px;}
.footer_top_wrap .footer_sns ul li.s3:hover {background-position: -62px -22px;}
.footer_top_wrap .footer_sns ul li.s4:hover {background-position: -93px -22px;}
.footer_top_wrap .footer_sns ul li.s5:hover {background-position: -124px -22px;}

/* 아이콘 이미지 최적화 작업 */
.icon {background-image: url(../img/icon_sns.png);}

.footer_top_wrap .familySite {float:right;}
.footer_top_wrap .familySite select {width:200px; height:30px; padding:5px; border:1px solid #999; background:#fff url('../img/select_arrow.png') no-repeat 95% 50%;}
.footer_top_wrap .familySite span {width:40px; height:30px; font-weight:600; font-size:0.778rem; color:#fff; letter-spacing:-1px; background:#333; vertical-align:middle; padding:5px;}
.footer_top_wrap .familySite span .btnGo { color:#fff; }

#footer_infor {width:100%; background:#f4f4f4;}
#footer_infor_wrap {width:1200px; text-align:center; margin:0 auto; position:relative; padding:40px 0;}
#footer_infor_wrap address p {margin-top:15px; font-size:0.889rem; white-space:pre-line;}
#footer_infor_wrap address p.ft_txt {font-size:0.778rem; color:#666; margin-top:10px; line-height:1.5;}
#footer_infor_wrap .ft_copy {font-size:0.778rem; margin-top:20px; color:#666;}
#footer_infor_wrap .ft_mark {position:absolute; bottom:10px; right:300px;}
#footer_infor_wrap .ft_mark > a > img {max-width: 90px;}


/* 미디어쿼리 */

@media all and (max-width:1500px){
    .Main_businessBg {right:66%;}
}

@media all and (max-width: 1290px){
    .header .nav ul li {width:93px;}
    .header .nav ul li a {font-size:0.833rem; padding:17px 10px;}
    .swiper-button-prev, .swiper-button-next {display:none;}
    .container {width:100%;}
}


@media all and (min-width:1200px) { /* pc ver */
    #header .headerInnr {width:98%;}
    #header .btnNav {display:none;}
    #header #topNav > .btnClose {display:none;}
    #header #topNav ul.topNavMenus {display:table; float:right;}
    #header #topNav ul.topNavMenus > li {float:left;}
    #header #topNav ul.topNavMenus > li > a {display:table; padding-left:36px; font-size:1.111rem; line-height:90px; text-decoration:none;}
    #header #topNav ul.topNavMenus > li > a > strong {position:relative; display:table-cell; vertical-align:middle;}
    #header #topNav ul.topNavMenus > li .topNavSub {display:none; position:absolute; top:100%; left:-495px; text-align:left; height:400px !important; width:900px; padding-left:500px; padding-right:300px;}
    #header #topNav ul.topNavMenus > li .btnMenuDropDown {display:none;}
    #header #topNav ul.topNavMenus > li:last-child > a:after {display:none;}
    #header #topNav ul.topNavMenus > li > a > strong:before {content:""; display:block; position:absolute; bottom:0; left:50%; width:0; height:2px; background:#3e8102; transition:all 0.2s;}
    #header #topNav ul.topNavMenus > li.active > a > strong:before,
    #header #topNav ul.topNavMenus > li > a:hover > strong:before,
    #header #topNav ul.topNavMenus > li > a:focus > strong:before {width:100%; left:0;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul:after {content:""; clear:both; display:block;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul {margin:5px 0 0 15px;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul > li {font-size:0.889rem; margin-bottom:4px;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul > li > ul {margin:2px 0 5px 5px;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul > li > ul > li {font-size:0.889rem;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul > li > ul > li:before {content:"-"; display:inline-block; vertical-align:top; margin-right:3px;}
    #header #topNav ul.topNavMenus > li .topNavSub > ul > li > ul > li > ul > li > ul {display: none;}
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li {float:left; width:400px; padding-top:20px; margin-right:10px; }
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li:nth-child(6n+1) {clear:left;}
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li > a {display:block; border:2px solid #ddd; padding:10px 10px; background:#FFF; font-size:0.889rem; margin-left:30px;}
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li > a:hover,
    #header #topNav ul.topNavMenus > li:not(.topNavArea) .topNavSub > ul > li > a:focus {border:2px solid #3e8102; font-weight:bold;}

    #header .topNavBackDv {display:block; position:absolute; left:0; width:100%; height:0; background:#fff; box-shadow:0 4px 4px -2px rgba(0,0,0,0.1);}
    #header .topNavBackDv .topNavTit {position:absolute; left:0; top:0; bottom:0; background:#3e8102; background-position:right -90px top 80px ;
    background-repeat:no-repeat; background-image:url(../img/dropMenu_logo.png);}
    #header .topNavBackDv .topNavTit h2 {color:#fff; font-size:2rem; line-height:1; margin-top:40px; padding-top:20px; position:absolute; top:0;}
    #header .topNavBackDv .topNavTit h2:before {content:""; display:block; top:0; left:0; position:absolute; width:30px; height:1px; background:#fff;}
    

    .Main_businessWrap {padding: 0 3%;} 
    .MainContent_notice {padding:0 3%;}

}


@media all and (max-width:1199px) { /* tablet & mobild common */
    body.opened-topNav {overflow-y:hidden;}

    #header .headerBody {height:90px; z-index:100; border-bottom:none; padding-left:2%;}
    #header .topNavBackDv .topNavTit h2 {display:none;}
    #header .btnNav {display:block; position:absolute; top:50%; right:0; transform:translateY(-50%);}
