<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>

	<definition name="/*/unit/*/*/*/*/" extends="{1}Sub">
		<put-attribute name="body" value="/{1}/jsp/unit/{2}/{3}/{4}/{5}.jsp" />
	</definition>
	<definition name="/*/unit/*/*/*/" extends="{1}Sub">
		<put-attribute name="body" value="/{1}/jsp/unit/{2}/{3}/{4}.jsp" />
	</definition>
	<definition name="/*/unit/*/*/" extends="{1}Sub">
		<put-attribute name="body" value="/{1}/jsp/unit/{2}/{3}.jsp" />
	</definition>

	<definition name="/empty/*/unit/*/*/" extends="portalEmpty">
		<put-attribute name="body" value="/{1}/jsp/unit/{2}/{3}.jsp" />
	</definition>

</tiles-definitions>